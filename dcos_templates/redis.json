{
  "id": "redis",
  "cpus": 0.5,
  "mem": 128,
  "disk": 512,
  "instances": 1,
  "container": {
    "type": "DOCKER",
    "docker": {
      "image": "redis",
      "network": "BRIDGE",
      "portMappings": [       
        {
          "containerPort": 6379,
          "hostPort": 0,
          "servicePort": 10000 
        }      
      ]
    }
  },
  "labels": {
    "HAPROXY_GROUP": "internal"
  }
}
